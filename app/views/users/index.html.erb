<h1>Listing users</h1>

<table>
  <tr>
    <th>Provider</th>
    <th>Uid</th>
    <th>Token</th>
    <th>Name</th>
    <th>Email</th>
    <th>Screenname</th>
    <th>Bio</th>
    <th>Religion</th>
    <th>Political</th>
    <th>Image</th>
    <th>Provider profile</th>
    <th>Role</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.provider %></td>
    <td><%= user.uid %></td>
    <td><%= user.token %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.screenname %></td>
    <td><%= user.bio %></td>
    <td><%= user.religion %></td>
    <td><%= user.political %></td>
    <td><%= user.image %></td>
    <td><%= user.provider_profile %></td>
    <td><%= user.role %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
